#include <iostream>
using namespace std;

void trocar(int &a, int &b) {
    int temp = a;
    a = b;
    b = temp;
}

int main() {
    int x, y;

    cout << "Digite dois numeros: ";
    cin >> x >> y;

    trocar(x, y);

    cout << "Depois da troca: x = " << x << ", y = " << y << endl;

    return 0;
}
