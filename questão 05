#include <iostream>
using namespace std;

// Função soma
int soma(int a, int b) {
    return a + b;
}

int main() {
    int x, y;

    // Entrada de dados
    cout << "Digite dois numeros inteiros: ";
    cin >> x >> y;

    // Saída do resultado
    cout << "Soma: " << soma(x, y) << endl;

    return 0;
}
